{
  "name": "TypeORM-Relations",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/lipex360x/TypeORM-Relations.git",
  "author": "Felipe Borges <lipex360@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "dev:server": "ts-node-dev -r tsconfig-paths/register --inspect --transpile-only --ignore-watch node_modules src/shared/infra/http/server.ts",

    "orm": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "orm:create": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:create -n ",
    "orm:run": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:run",
    "orm:revert": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:revert",
    "orm:show": "ts-node-dev -r tsconfig-paths/register ./node_modules/typeorm/cli.js migration:show",
    
    "test": "jest --passWithNoTests --silent --noStackTrace --setupFiles dotenv/config",
    "test:c": "jest --clearCache",
    "test:v": "jest --passWithNoTests"
  },
  "dependencies": {
    "celebrate": "^13.0.4",
    "class-transformer": "^0.3.1",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "tracer": "^1.1.4",
    "tsyringe": "^4.4.0",
    "typeorm": "^0.2.29",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.15",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.1.2"
  }
}
